{
  "name": "universal-music-distribution-protocol",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Gordo",
  "license": "MIT",
  "homepage": "https://songs-tools.com",
  "keywords": [
    "hardhat",
    "ethereum",
    "music",
    "NFT"
  ],
  "scripts": {
    "test": "npx hardhat test",
    "serve": "npx hardhat node",
    "deploy:songs": "ts-node scripts/run-with-network.ts deploy-songs",
    "deploy:one": "ts-node scripts/run-with-network.ts deploy-one",
    "deploy:protocol": "ts-node scripts/run-with-network.ts deploy-protocol",
    "deploy:protocol-main": "ts-node scripts/run-with-network.ts deploy-protocol-main",
    "deploy:license": "ts-node scripts/run-with-network.ts deploy-license",
    "deploy": "ts-node scripts/run-with-network.ts deploy",
    "deploy:ws-2": "ts-node scripts/run-with-network.ts deploy-ws-2",
    "deploy:all": "ts-node scripts/run-with-network.ts deploy-all",
    "compile": "npx hardhat compile",
    "size": "yarn run hardhat size-contracts",
    "console": "npx hardhat console --network localhost",
    "remix": "remixd -s ./ --remix-ide https://remix.ethereum.org",
    "subgraph:codegen": "yarn subgraph:prepare && cd subgraph && graph codegen",
    "subgraph:build": "cd subgraph && yarn build",
    "subgraph:deploy": "cd subgraph && yarn deploy",
    "subgraph:prepare": "ts-node scripts/update-subgraph-config.ts",
    "verify:baseSepolia": "npx hardhat compile && npx hardhat run scripts/verify-protocol.ts --network baseSepolia",
    "verify:base": "npx hardhat compile && npx hardhat run scripts/verify-protocol.ts --network base"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gordo-labs/Universal-Music-Royalties/protocol.git"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.7",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-verify": "^2.0.8",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "@openzeppelin/hardhat-upgrades": "^3.2.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.11",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "@types/react-color": "^3.0.5",
    "chai": "4.5.0",
    "env-cmd": "^10.1.0",
    "eth-gas-reporter": "^0.2.27",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^6.11.1",
    "hardhat": "^2.22.7",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.10",
    "husky": "^9.0.11",
    "js-yaml": "^4.1.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.68.0",
    "@graphprotocol/graph-ts": "^0.32.0",
    "@openzeppelin/contracts": "^5.1.0",
    "@remix-project/remixd": "^0.6.33",
    "dotenv": "^16.4.5"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
